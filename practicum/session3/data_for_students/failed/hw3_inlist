
&star_job

      show_log_description_at_start = .false.
      save_model_when_terminate = .false.
      save_model_filename = 'final.mod'
      save_star_job_namelist = .false.
      star_job_namelist_name = 'test'

      !this helps the initial model to converge
      create_pre_main_sequence_model = .true.
      pre_ms_T_c = 7d5

      kappa_file_prefix = 'gs98'

      pre_ms_relax_num_steps = 50
      pgstar_flag = .true.

      history_columns_file = 'hw3_history_columns.list'

/ ! end of star_job namelist

&controls


    ! output directory for log files
      log_directory = 'M15_HW3'

      initial_mass = 15.0
      initial_z = 0.02d0
 
    ! mass-loss prescription
    cool_wind_RGB_scheme = 'Dutch'
    cool_wind_AGB_scheme = 'Dutch'
    hot_wind_scheme = 'Dutch'

    Dutch_scaling_factor = 1.0
    max_wind = 1d-3
    hot_wind_full_on_T = 0d0
    cool_wind_full_on_T = 0d0


    ! force the maximum timestep to be this value
      max_years_for_timestep = 1d5

      use_Type2_opacities = .true.
      Zbase = 0.02d0

      use_gold_tolerances = .true.

 
    ! check for retries and backups as part of test_suite
    ! you can/should delete this for use outside of test_suite
      max_number_backups = 20
      max_number_retries = 500
      max_model_number = 25000

      photo_interval = 50
      profile_interval = 50
      history_interval = 1
      terminal_interval = 10
      write_header_frequency = 10

      okay_to_reduce_gradT_excess = .true. 
      Pextra_factor = 2.0
  
      mixing_length_alpha = 1.8
      MLT_option = 'Henyey'

      delta_lgL_He_limit = 0.1 ! limit for magnitude of change in lgL_He

      mesh_delta_coeff = 0.5
      varcontrol_target = 1d-3
      max_allowed_nz = 50000
	  
      !better resolution of the Henyey hook
      delta_lg_XH_cntr_max = -1
		 
      !increase resolution
      mesh_logX_species(1) = 'h1'
      mesh_logX_min_for_extra(1) = -6
      mesh_dlogX_dlogP_extra(1) = 0.25
      
      mesh_logX_species(2) = 'he4'
      mesh_logX_min_for_extra(2) = -6
      mesh_dlogX_dlogP_extra(2) = 0.25
	  
      mesh_logX_species(3) = 'c12'
      mesh_logX_min_for_extra(3) = -6
      mesh_dlogX_dlogP_extra(3) = 0.25
	  
      use_Ledoux_criterion = .true.
      alpha_semiconvection = 0.1
      thermohaline_coeff = 666.0
      thermohaline_option = 'Kippenhahn'

      ! overshoot params for top boundary of convective core, all core types
      overshoot_scheme(2)   = 'exponential'
      overshoot_zone_type(2)= 'any'
      overshoot_zone_loc(2) = 'core'
      overshoot_bdy_loc(2)  = 'top'
      overshoot_f(2)        = 0.016
      overshoot_f0(2)       = 0.008

      ! Shells: same f/f0 at top & bottom, for nonburn + H/He/Z shells
      overshoot_scheme(1)   = 'exponential'
      overshoot_zone_type(1)= 'any'
      overshoot_zone_loc(1) = 'shell'
      overshoot_bdy_loc(1)  = 'any'
      overshoot_f(1)        = 0.0174
      overshoot_f0(1)       = 0.0087


/ ! end of controls namelist



